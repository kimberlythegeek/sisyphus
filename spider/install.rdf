<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:em="http://www.mozilla.org/2004/em-rdf#">

        <Description about="urn:mozilla:install-manifest">

                <em:id>{47b978aa-2fbb-4f64-886f-b2845d76c709}</em:id>
                <em:name>Spider</em:name>
                <em:version>0.0.2.23</em:version>
                <em:description>A Web Spider for Firefox, and Gecko applications</em:description>
                <em:creator>Bob Clary</em:creator>
                <em:homepageURL>http://bclary.com/projects/spider/</em:homepageURL>
                <!--
                <em:optionsURL>chrome://spider/content/settings.xul</em:optionsURL>
                <em:aboutURL>chrome://spider/content/about.xul</em:aboutURL>
                <em:iconURL>chrome://spider/skin/mainicon.png</em:iconURL>
                -->
                <!-- no longer provide updates since the extension is disabled on the
                trunk otherwise
                <em:updateURL>http://bclary.com/projects/spider/update.rdf</em:updateURL>
                -->
                <em:type>2</em:type> <!-- type=extension -->

    <!-- Firefox -->
                <em:targetApplication>
                        <Description>
                                <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
                                <em:minVersion>1.0</em:minVersion>
                                <em:maxVersion>3.7</em:maxVersion>
                        </Description>
                </em:targetApplication>

    <!-- Thunderbird -->
                <em:targetApplication>
                        <Description>
                                <em:id>{3550f703-e582-4d05-9a08-453d09bdfdc6}</em:id>
                                <em:minVersion>1.0</em:minVersion>
                                <em:maxVersion>3.2</em:maxVersion>
                        </Description>
                </em:targetApplication>

    <!-- Mozilla Application Suite -->
                <em:targetApplication>
                        <Description>
                                <em:id>{86c18b42-e466-45a9-ae7a-9b95ba6f5640}</em:id>
                                <em:minVersion>1.7</em:minVersion>
                                <em:maxVersion>2.0</em:maxVersion>
                        </Description>
                </em:targetApplication>

    <!-- SeaMonkey -->
                <em:targetApplication>
                        <Description>
                                <em:id>{92650c4d-4b8e-4d2a-b7eb-24ecf4f6b63a}</em:id>
                                <em:minVersion>1.0</em:minVersion>
                                <em:maxVersion>2.0</em:maxVersion>
                        </Description>
                </em:targetApplication>

    <!-- Fennec -->
                <em:targetApplication>
                        <Description>
                                <em:id>{a23983c0-fd0e-11dc-95ff-0800200c9a66}</em:id>
