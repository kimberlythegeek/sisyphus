                </em:targetApplication>

    <!--
    This is not needed for Firefox 1.1 and later. Only include this
    if you want to make your extension compatible with older versions
    -->
                <em:file>
                        <Description about="urn:mozilla:extension:file:spider.jar">
        <em:package>content/spider/</em:package>
                                <!-- optional items -->
        <em:skin>skin/classic/spider/</em:skin>
        <em:skin>skin/modern/spider/</em:skin>
        <em:locale>locale/en-US/spider/</em:locale>
                        </Description>
                </em:file>
        </Description>

</RDF>
